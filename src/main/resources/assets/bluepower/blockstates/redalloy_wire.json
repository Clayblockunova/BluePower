{
  "multipart": [
    { "when": { "powered": "false"}, "apply": {"model": "bluepower:block/cable/alloywire_centre" }},
    { "when": { "powered": "true"}, "apply": {"model": "bluepower:block/cable/alloywire_centre_on" }},
    { "when": { "connected_front": "true", "powered": "false"}, "apply": { "model": "bluepower:block/cable/alloywire_extended","y": -90 }},
    { "when": { "connected_back": "true", "powered": "false"}, "apply": { "model": "bluepower:block/cable/alloywire_extended","y": 90 }},
    { "when": { "connected_left": "true", "powered": "false"}, "apply": { "model": "bluepower:block/cable/alloywire_extended","y": 180 }},
    { "when": { "connected_right": "true", "powered": "false"}, "apply": { "model": "bluepower:block/cable/alloywire_extended" }},
    { "when": { "connected_front": "true", "powered": "true"}, "apply": { "model": "bluepower:block/cable/alloywire_extended_on","y": -90 }},
    { "when": { "connected_back": "true", "powered": "true"}, "apply": { "model": "bluepower:block/cable/alloywire_extended_on","y": 90 }},
    { "when": { "connected_left": "true", "powered": "true"}, "apply": { "model": "bluepower:block/cable/alloywire_extended_on","y": 180 }},
    { "when": { "connected_right": "true", "powered": "true"}, "apply": { "model": "bluepower:block/cable/alloywire_extended_on" }}
  ]
}
